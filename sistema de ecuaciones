#include "stdafx.h"
#include <iostream>
#include "conio.h"
#include "stdlib.h"

　
using namespace std;

void leer1(float A, float B, float C);
void solucion1(float A, float B, float C, float D, float E, float f, float &X, float &Y);

　
void main()
{
	float A,B,C,D,E,F,X,Y;

	do{
		cout<<"\n Ingrese el valor de A!= 0: ";
		cin>> A;
	} while (A== 0);
	cout<<"\n Ingrese el valor de B: ";
	cin>> B;

	cout<<"\n Ingrese el valor de C: ";
	cin>> C;

	cout<<"\n Ingrese el valor de D: ";
	cin>> D;

	cout<<"\n Ingrese el valor de E: ";
	cin>> E;

	cout<<"\n Ingrese el valor de F: ";
	cin>> F;

	solucion1(A,B,C,D,E,F,X,Y);
	cout<<"\n El valor de X es igual "<< X;
	cout<<"\n El valor de Y es igual "<< Y;
	_getch();

}

void solucion1(float O, float P, float Q, float R, float S, float T, float &M, float &N)
{ M = (( O * T) - ( R * Q )) / (( O * S) - ( R * P ));
  N = ( O -P * N) / O;
  Q = 7;
}
